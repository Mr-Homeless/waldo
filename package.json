{
  "name": "waldo.vision",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "db": "docker compose up",
    "dev": "turbo run dev",
    "clean": "turbo run clean",
    "checks": "turbo run build lint",
    "build": "turbo run build",
    "start": "turbo run build start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "eslint": "latest",
    "eslint-config-custom": "workspace:0.0.0",
    "prettier": "latest",
    "tsconfig": "workspace:0.0.0",
    "turbo": "latest",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.15.0"
}
