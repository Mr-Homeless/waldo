{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^db:generate", "^build"],
      "env": [
        "PORT",
        "HOST",
        "DB_URL",
        "FS_LOCATION",
        "NODE_ENV",
        "OTLP_COLLECTOR_BASE"
      ],
      "outputs": ["dist/**", ".next/**", "out/**"]
    },
    "dev": {
      "dependsOn": ["^db:generate"],
      "cache": false
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "start": {
      "dependsOn": ["^build"],
      "cache": true,
      "env": [
        "PORT",
        "HOST",
        "DB_URL",
        "FS_LOCATION",
        "NODE_ENV",
        "OTLP_COLLECTOR_BASE"
      ]
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:push:fdl": {
      "cache": false
    }
  },
  "globalEnv": ["NODE_ENV"]
}
