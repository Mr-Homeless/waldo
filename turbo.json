{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "env": ["PORT", "HOST", "DB_URL", "FS_LOCATION", "NODE_ENV"],
      "outputs": ["dist/**", ".next/**", "out/**"]
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "start": {
      "dependsOn": ["^build"],
      "cache": true,
      "env": ["PORT", "HOST", "DB_URL", "FS_LOCATION", "NODE_ENV"]
    }
  }
}
