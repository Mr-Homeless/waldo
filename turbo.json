{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "env": ["PORT", "HOST", "DB_URL"],
      "outputs": ["dist/**", ".next/**", "out/**"]
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false,
      "dependsOn": ["^build"],
      "env": ["PORT", "HOST", "DB_URL"]
    }
  }
}
